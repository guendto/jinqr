{
  "name": "jinqr",
  "type": "module",
  "version": "0.1.4",
  "description": "The simple jomiel client for downloading media streams",
  "keywords": [
    "jomiel",
    "media",
    "downloader"
  ],
  "repository": "github:guendto/jinqr",
  "author": "Toni Gündoğdu",
  "license": "Apache-2.0",
  "engine": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "main": "src/main.js",
  "bin": "src/main.js",
  "man": [
    "man/jinqr.1.gz"
  ],
  "files": [
    "src",
    "man"
  ],
  "scripts": {
    "release": "standard-version -s --releaseCommitMessageFormat 'chore(release): jinqr version {{currentTag}} (tG)'",
    "prepack": "mkdir -p man && remark docs/man/jinqr.1.md -u remark-man -o man/jinqr.1 && gzip -f man/jinqr.1",
    "build-jsdoc": "rimraf docs/jsdoc/ && jsdoc --pedantic -c ./jsdoc.json .",
    "distclean": "rimraf docs/jsdoc man node_modules"
  },
  "dependencies": {
    "@sindresorhus/slugify": "^2.2.1",
    "camelcase": "^8.0.0",
    "cli-progress": "^3.12.0",
    "cli-spinners": "^2.9.1",
    "cli-truncate": "^3.1.0",
    "console-table-printer": "^2.11.2",
    "convert-hrtime": "^5.0.0",
    "filenamify": "^6.0.0",
    "global-agent": "^3.0.0",
    "got": "^13.0.0",
    "humanize-url": "^3.0.0",
    "jomiel-messages": "^0.2.1",
    "js-yaml": "^4.1.0",
    "log4js": "^6.9.1",
    "mime-types": "^2.1.35",
    "ora": "^7.0.1",
    "picocolors": "^1.0.0",
    "pretty-bytes": "^6.1.1",
    "pretty-ms": "^8.0.0",
    "protobufjs": "^7.2.5",
    "pupa": "^3.1.0",
    "simple-eta": "^3.0.2",
    "ua-string": "^4.0.0",
    "untildify": "^5.0.0",
    "yargs": "^17.7.2",
    "zeromq": "6.0.0-beta.17"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/eslint-parser": "^7.22.15",
    "eslint": "^8.50.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "jsdoc": "^4.0.2",
    "prettier": "^3.0.3",
    "remark-cli": "^12.0.0",
    "remark-man": "^9.0.0",
    "rimraf": "^5.0.5",
    "standard-version": "^9.5.0"
  },
  "pnpm": {
    "overrides": {
      "semver@>=7.0.0 <7.5.2": ">=7.5.2",
      "json5@>=2.0.0 <2.2.2": ">=2.2.2",
      "minimatch@<3.0.5": ">=3.0.5",
      "minimist@>=1.0.0 <1.2.6": ">=1.2.6"
    }
  }
}
